package com.torc.ui.demo.base;

import org.springframework.beans.factory.annotation.Autowired;
import com.torc.ui.demo.page.CartPage;
import com.torc.ui.demo.page.HomePage;
import com.torc.ui.demo.page.ProductsPage;
import com.torc.ui.demo.page.components.MenuBar;

public abstract class BaseSteps{

	@Autowired
	protected HomePage loginPage;

	@Autowired
	protected ProductsPage productsPage;

	@Autowired
	protected CartPage shoppingCartPage;
	
	@Autowired
	protected MenuBar menu;
}
